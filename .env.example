# 环境变量配置模板
# 复制此文件为 .env 并填入你的实际值

# === API 密钥 ===
QWEN_API_KEY=your_qwen_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# === Gemini 配置 ===
GEMINI_MODEL_ID=your_gemini_modelname_here
GEMINI_TIMEOUT=300

# === 可选配置 ===
# 是否启用调试模式
DEBUG=false

# 日志级别 (INFO, DEBUG, WARNING, ERROR)
LOG_LEVEL=INFO

# 预测保留天数（自动清理）
PREDICTION_RETENTION_DAYS=90

# 备份保留天数
BACKUP_RETENTION_DAYS=7
